{
  "name": "it-doc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^5.21.6",
    "caniuse-lite": "^1.0.30001677",
    "dayjs": "^1.11.11",
    "formik": "^2.2.6",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-awesome-reveal": "^3.7.0",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.43.2",
    "styled-components": "^5.3.3",
    "swiper": "^6.5.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^12.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.15",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .jsx,.ts,.tsx",
    "prettier": " prettier \"src/**/*.{ts,tsx}\"",
    "prettier:write": " prettier --write \"src/**/*.{ts,tsx}\"",
    "openapi:download": "dotenv -- cross-var curl \"%REACT_APP_OPENAPI_URL%\" -o ./src/shared/types/api/openapi.json",
    "openapi:generate": "openapi-generator-cli generate -i ./src/shared/types/api/openapi.json -g typescript-axios --skip-validate-spec --enable-post-process-file -o ./src/shared/types/api/generated",
    "openapi": "yarn openapi:download && yarn openapi:generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
